{
  "name": "@booking-group-workspace/fe-react-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/fe-react-web/src",
  "tags": ["scope:fe-react-web", "type:app"],
  "targets": {
    "build": {
      "outputs": ["{projectRoot}/{options.outDir}", "{projectRoot}/dist"],
      "defaultConfiguration": "production",
      "options": {
        "outDir": "../../dist/apps/fe-react-web"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "defaultConfiguration": "development",
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "preview": {
      "defaultConfiguration": "development",
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/fe-react-web/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "apps/fe-react-web"
      }
    },
    "test:ui": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/apps/fe-react-web"],
      "options": {
        "command": "vitest",
        "cwd": "apps/fe-react-web"
      },
      "configurations": {
        "ui": {
          "command": "vitest --ui"
        }
      }
    }
  }
}
