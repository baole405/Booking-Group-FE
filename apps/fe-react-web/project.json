{
  "name": "@booking-group-workspace/fe-react-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/fe-react-web/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/fe-react-web"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "fe-react-web:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "fe-react-web:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "fe-react-web:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "fe-react-web:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "fe-react-web:build:development"
        },
        "production": {
          "buildTarget": "fe-react-web:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/fe-react-web/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "apps/fe-react-web"
      }
    }
  },
  "tags": ["scope:fe-react-web", "type:app"]
}
